<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity_landscape">

    <TextView
            android:id="@+id/resultField"
            android:layout_width="697dp"
            android:layout_height="55dp"
            android:textSize="48sp"
            android:backgroundTint="@android:color/darker_gray"
            android:background="#782E2E"
            android:textColor="@android:color/background_dark"
            android:textAlignment="textEnd"
            tools:layout_editor_absoluteY="7dp"
            tools:layout_editor_absoluteX="18dp"/>
    <EditText
            android:id="@+id/numberField"
            android:layout_width="495dp"
            android:layout_height="53dp"
            android:inputType="number|phone"
            android:textSize="48sp"
            android:backgroundTint="@android:color/darker_gray"
            android:background="#782E2E"
            android:textColor="@android:color/background_dark"
            android:textAlignment="textEnd"
            tools:ignore="MissingConstraints"
            android:autofillHints=""
            tools:layout_editor_absoluteY="82dp"
            app:layout_constraintStart_toEndOf="@+id/operationField"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:id="@+id/operationField"
            android:layout_width="183dp"
            android:layout_height="54dp"
            android:textSize="48sp"
            android:backgroundTint="@android:color/darker_gray"
            android:background="#782E2E"
            android:textColor="@android:color/background_dark"
            android:textAlignment="textEnd"
            tools:layout_editor_absoluteY="82dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/numberField"/>
    <Button
            android:id="@+id/button2"
            android:text="2"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="144dp"
            app:layout_constraintStart_toEndOf="@+id/button" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/button3"/>
    <Button
            android:id="@+id/button3"
            android:text="3"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="144dp"
            app:layout_constraintStart_toEndOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button4"/>
    <Button
            android:id="@+id/button4"
            android:text="+"
            android:onClick="onOperationClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#8BC34A" tools:layout_editor_absoluteY="144dp"
            app:layout_constraintStart_toEndOf="@+id/button3"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button12"/>
    <Button
            android:id="@+id/button6"
            android:text="5"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="232dp"
            app:layout_constraintStart_toEndOf="@+id/button5" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/button7"/>
    <Button
            android:id="@+id/button7"
            android:text="6"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="232dp"
            app:layout_constraintStart_toEndOf="@+id/button6"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button8"/>
    <Button
            android:id="@+id/button8"
            android:text="-"
            android:onClick="onOperationClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#8BC34A" tools:layout_editor_absoluteY="235dp"
            app:layout_constraintStart_toEndOf="@+id/button7"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button15"/>
    <Button
            android:id="@+id/button10"
            android:text="8"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="321dp"
            tools:layout_editor_absoluteX="115dp"/>
    <Button
            android:id="@+id/button11"
            android:text="9"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="321dp"
            tools:layout_editor_absoluteX="213dp"/>
    <Button
            android:id="@+id/button12"
            android:text="*"
            android:onClick="onOperationClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#8BC34A" tools:layout_editor_absoluteY="144dp"
            app:layout_constraintStart_toEndOf="@+id/button4"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button16"/>
    <Button
            android:id="@+id/button14"
            android:text="."
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="321dp"
            tools:layout_editor_absoluteX="408dp"/>
    <Button
            android:id="@+id/button15"
            android:text="/"
            android:onClick="onOperationClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#8BC34A" tools:layout_editor_absoluteY="235dp"
            app:layout_constraintStart_toEndOf="@+id/button8"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button17"/>
    <Button
            android:id="@+id/button16"
            android:text="C"
            android:onClick="onOperationClick"
            android:layout_width="208dp"
            android:layout_height="78dp"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#FF9800" tools:layout_editor_absoluteY="144dp"
            tools:layout_editor_absoluteX="506dp"/>
    <Button
            android:id="@+id/button"
            android:text="1"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/button2" app:layout_constraintTop_toBottomOf="@+id/operationField"
            app:layout_constraintBottom_toTopOf="@+id/button5"/>
    <Button
            android:id="@+id/button5"
            android:text="4"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button6"
            app:layout_constraintTop_toBottomOf="@+id/button" app:layout_constraintBottom_toTopOf="@+id/button9"/>
    <Button
            android:id="@+id/button9"
            android:text="7"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4"
            tools:layout_editor_absoluteY="321dp" tools:layout_editor_absoluteX="18dp"/>
    <Button
            android:id="@+id/button13"
            android:text="0"
            android:onClick="onNumberClick"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#00BCD4" tools:layout_editor_absoluteY="321dp"
            tools:layout_editor_absoluteX="310dp"/>
    <Button
            android:id="@+id/button17"
            android:text="="
            android:onClick="onOperationClick"
            android:layout_width="207dp"
            android:layout_height="77dp"
            android:textSize="@dimen/button_textSize"
            android:textStyle="bold"
            android:backgroundTint="#FF9800" tools:layout_editor_absoluteY="235dp"
            tools:layout_editor_absoluteX="506dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>